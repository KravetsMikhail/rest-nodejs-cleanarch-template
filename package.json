{
  "name": "rest-nodejs-cleanarch-template",
  "version": "0.0.1",
  "description": "Шаблон REST API сервера на чистой архитектуре",
  "main": "server.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "concurrently \"tsc --watch\" \"nodemon dist/index.js\"",
    "build": "tsc",
    "serve": "node dist/index.js",
    "test": "jest --watch",
    "copySpecialFiles": "copyfiles --error --up 1 src/api/**/*.yaml* dist",
    "postbuild": "npm run copySpecialFiles",
    "oraclemigrate": "node src/api/v1/infrastructure/oracle/migration.js"
  },
  "author": "Kravets Mikhail",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "compression": "^1.7.4",
    "cookie": "^0.7.2",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.1",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "marv": "^6.1.0",
    "marv-oracledb-driver": "^2.0.0",
    "nodemon": "^3.1.7",
    "oracledb": "^6.6.0",
    "path": "^0.12.7",
    "pg": "^8.13.0",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@types/oracledb": "^6.5.2",
    "@types/pg": "^8.11.10",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/serve-favicon": "^2.5.7",
    "concurrently": "^9.0.1",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  }
}
